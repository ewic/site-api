{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3030,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "queue_directory": "queue",
  "file_directory": "files",
  "accepted_extensions": ["jpg", "jpeg", "png", "gif"]
}
